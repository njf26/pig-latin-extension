{
"manifest_version": 2,
    "name": "Inkoay",
    "description": "Convert any website into pig-latin",
    "version": "1.0",
    "page_action": {
      "default_icon": "logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
